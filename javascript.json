	"XmlHttpRequest_Local_joserafa": {
		"prefix": "httpRequest_Local",
		"body": [
			"/* XMLHTTPREQUEST LOCAL */",
			"'use strict';",
			"const file = '${1:ArchivoJson}.json';",
			"const request = new XMLHttpRequest();",
			"request.overrideMimeType('application/json'); //Local",
			"request.open('GET', file, true);",
			"request.onload = function () {",
			"\t var dato = JSON.parse(request.responseText);",
			"\t var HTML = '';",
			" for (var i = 0; i < dato.${2:ObjetoBuscar}.length; i++) {",
			"   var n_telefonos = ''; //Es un Arreglo de Objetos Sub-Busqueda",
			"   for (var j = 0; j < dato.${2:ObjetoBuscar}[i].telefonos.length; j++) {",
			"     n_telefonos += ` ${dato.${2:ObjetoBuscar}[i].telefonos[j].tipo}:",
			"                               ${dato.${2:ObjetoBuscar}[i].telefonos[j].numero}/`;",
			"   } //direccion",
			"   HTML += `<div class='book-card ' ><h4>Nombe: ${dato.${2:ObjetoBuscar}[i].nombre} Apellido:${dato.${2:ObjetoBuscar}[i].apellido}",
			"       Dir:${dato.${2:ObjetoBuscar}[i].direccion.calle},",
			"       ${dato.${2:ObjetoBuscar}[i].direccion.poblacion},",
			"       ${dato.${2:ObjetoBuscar}[i].direccion.provincia}-",
			"       Contacto:(${ n_telefonos})</h4></div>`;",
			"   muestraResultado(HTML);",
			" }",
			"};",
			"request.onerror = (error) => muestraResultado(`<pre>ERROR: ${error} <pre>`);",
			"request.send();",
			"const muestraResultado = (texto) =>",
			" (document.getElementById('output').innerHTML = texto);",
		],
		"description": "Crea objeto  Request para llamada Local."
	}
