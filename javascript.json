	"CuentaLosDias_Joserafa": {
		"prefix": "cuentaDias_jr",
		"body": [	
		"/* CUENTA REGRESIVA PARA EVENTO. */",
			"function cuentaDias(ano,mes,dia,nombreEvento) {",
			" const hoy = new Date();",
			" const milSegEnUnDia = 1000*60*60*24;//Milisegundo en un Dia",
			" hoy.setHours(0,0,0,0,);",
			" const fechaEvento = new Date(ano,mes -1,dia);",
			" const diferenciaHoras  = Math.abs(fechaEvento.getTimezoneOffset()/60)-Math.abs(hoy.getTimezoneOffset()/60);",
			" hoy.setHours(hoy.getHours()-diferenciaHoras);",	
		" if (fechaEvento>hoy) {",
			"   return`falta ${(fechaEvento.getTime()-hoy.getTime())/milSegEnUnDia} Dias para ${nombreEvento}`;",
			" }else{",
			"   return 'La Fecha ya Pasò'",
			" }",
			"}",
			"//console.log(cuentaDias(2041,11,10,'Cumple de Graciela'));"
		],
		"description": "Crea Función con cuenta Regresiva de Evento"
	}
